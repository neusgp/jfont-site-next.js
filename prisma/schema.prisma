// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model repertori {
  id   Int                   @id @default(autoincrement())
  genre String
  title String
  composer String
  place_year String
  
}

model agenda {
  id   Int                   @id @default(autoincrement())
  title String
  start String
  end String
  time String
  location String
  description String
  url String
}
